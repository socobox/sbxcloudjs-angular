{
  "name": "sbx",
  "version": "1.3.1",
  "description": "Sbx Core for Angular4",
  "keywords": [
    "angular 4",
    "sbx"
  ],
  "main": "index.js",
  "scripts": {
    "clean": "rimraf .tmp && rimraf dist",
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "./node_modules/uglify-js/bin/uglifyjs dist/bundles/sbx.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/sbx.umd.min.js",
    "copy": "cpx './package-dist.json' dist && renamer --find 'package-dist.json' --replace 'package.json' ./dist/*",
    "build": "npm run clean && npm run transpile && npm run package && npm run minify && npm run copy",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sbxcloud/angular4library.git"
  },
  "devDependencies": {
    "@angular/cli": "1.3.1",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/chai": "^4.1.2",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "~6.0.60",
    "chai": "^4.1.2",
    "codelyzer": "~3.1.1",
    "cpx": "^1.5.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^5.0.1",
    "protractor": "~5.1.2",
    "renamer": "^0.6.1",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.6",
    "ts-node": "^3.2.2",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "uglify-js": "2.8.29",
    "webpack-bundle-analyzer": "^2.9.1"
  },
  "dependencies": {
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "core-js": "^2.4.1",
    "ngx-cookie-service": "^1.0.7",
    "rxjs": "^5.4.2",
    "sbx-querybuilder": "^1.0.8",
    "sbxcore": "^1.0.2",
    "zone.js": "^0.8.14"
  }
}
